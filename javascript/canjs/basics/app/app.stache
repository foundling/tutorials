<input can-blur="addPerson"/>
<h1>Count: {{count}} </h1>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Length</th>
        </tr>
    </thead>
    <tbody>
    {{#each people}}
        <tr>
            <td>{{name}}</td>
            <td>{{charCount .}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>

